NUXT_PUBLIC_MAILSERVER=
NUXT_PUBLIC_MAILER_AUTH_USER=
NUXT_PUBLIC_MAILER_AUTH_PASS=
NUXT_PUBLIC_MAILER_FROM_MAILADDRESS=